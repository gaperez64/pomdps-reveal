INFO {
  TITLE:       "Space_shuttle - hard"
  DESCRIPTION: "LTL specification extracted from ltl-Space_shuttle - hard.pomdp"
  SEMANTICS:   Moore
  TARGET:      Moore
}

MAIN {

  INPUTS {
    p0;
    p1;
  }

  OUTPUTS {
    action;
  }

  GUARANTEES {
    // Specification 1
    GF p0;

    // Specification 2
    G !p1;

    // Specification 3
    GF p0 && G !p1;

    // Specification 4
    F G p0;
  }
}
